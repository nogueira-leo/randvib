   "metadata": {},
   "outputs": [],
   "source": [
    "n = 10000\n",
    "A = 2\n",
    "w = 1\n",
    "theta = np.random.uniform(size=n)*2*np.pi\n",
    "x1 = A*np.sin(theta)\n",
    "t = np.linspace(0,2*np.pi,n)\n",
    "x2 = A*np.sin(w*t)\n",
    "plt.figure()\n",
    "plt.hist(x1,20)\n",
    "plt.figure()\n",
    "plt.hist(x2,20)"
   ]
  }
